{
  "name": "@plugnrent/client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@plugnrent/common": "1.0.0",
    "@plugnrent/controller": "1.0.0",
    "antd": "^3.6.2",
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-link-http": "^1.5.4",
    "formik": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "yup": "^0.25.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
